   "source": [
    "def fibonacci(n):\n",
    "    l = []\n",
    "    n1 = 0\n",
    "    n2 = 1\n",
    "    if n < 0:\n",
    "        print(\"Wrong input\")\n",
    "    for i in range(n):\n",
    "        l.append(n1)\n",
    "        roll = n1 + n2\n",
    "        n1 = n2\n",
    "        n2 = roll\n",
    "    return l\n",
    "        \n",
    "        "
   ]
